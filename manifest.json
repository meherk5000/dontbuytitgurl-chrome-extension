{
    "manifest_version": 3,
    "name": "DontBuyItGurl",
    "version": "1.0",
    "permissions": ["activeTab", "scripting"],
    "host_permissions": ["<all_urls>"],
    "background": {
      "service_worker": "background.js" 
    },
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["pop-up.js"],
        "run_at": "document_idle"
      }
    ]
  }
  